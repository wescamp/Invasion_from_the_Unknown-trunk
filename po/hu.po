#: Invasion_from_the_Unknown/macros/traits.cfg:10
msgid "spirit"
msgstr "lélek"

#. [trait]: id=spirit
#: Invasion_from_the_Unknown/macros/traits.cfg:11
msgid "female^spirit"
msgstr "lélek"

