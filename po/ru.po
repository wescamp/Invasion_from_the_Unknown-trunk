#~ msgstr "Люди-варвары"

#~ msgid "team_name^Orcish barbarians"
#~ msgstr "Орки-варвары"

#~ msgid "team_name^Roaming ghosts"
#~ msgstr "Призраки-скитальцы"

#~ msgid "team_name^Roaming creatures"
#~ msgstr "Бродячие твари"

