#: Invasion_from_the_Unknown/units/monsters/NPC_Rabbit.cfg:26
msgid "incisors"
msgstr ""

#: Invasion_from_the_Unknown/units/fairies/elynia/glamour-variations/_final.cfg:118
msgid "elynia_glamour+female^Imp"
msgstr ""

#: Invasion_from_the_Unknown/units/fairies/elynia/glamour-variations/_final.cfg:126
msgid "It may look like a treacherous Imp, but it isn't."
msgstr ""

